<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ParkSeryu.munanmunan">

    <application
        android:allowBackup="true"
        android:icon="@drawable/witness_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/witness_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 스플래시 액티비티 -->
        <activity
            android:name="com.ParkSeryu.munanmunan.SplashActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="fullSensor">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.ParkSeryu.munanmunan.MainActivity"></activity>
    </application>

</manifest>