<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ParkSeryu.munanmunan">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/witness_icon"
        android:label="@string/app_name"

        android:roundIcon="@drawable/witness_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <!--android:debuggable="true"-->
        <!-- Sample AdMob App ID: ca-app-pub-3940256099942544~3347511713 -->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3814337593721451~4016822393" />
        <!-- 스플래시 액티비티 -->
        <activity
            android:name="com.ParkSeryu.munanmunan.SplashActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="fullSensor">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.ParkSeryu.munanmunan.MainActivity"/>
    </application>

</manifest>